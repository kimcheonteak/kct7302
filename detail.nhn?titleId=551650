	function init_socialplugin() { 
	    var d = document.head ? document.head : document.getElementsByTagName("head")[0]; 
	    var s = document.createElement("script"); 
	    var today = new Date(); 
	    var dd = today.getDate(); 
	    var mm = today.getMonth()+1; 
	    var yyyy = today.getFullYear(); 
	    if (dd < 10) { dd = '0' + dd; } 
	    if (mm < 10) { mm = '0' + mm; } 
	    s.src = ("http://spi.naver.net/js/release/socialplugin_v1.js?" + yyyy + mm + dd); 
	    s.charset = "utf-8"; 
	    d.appendChild(s); 
	}
	
	function draw_socialplugin() {
		var spi = new SocialPlugIn();
		spi.drawSpiLayer(htInitData, "spiLayer");
	} 

	/**
	 * 북스토어로 이동 가능한지 확인해서 이동
	 *
	 * @params nbooksId {String} 북스 상품 ID
	 */
	function moveBookstore(nbooksId) {
		 var result;
		$Ajax('http://nstore.naver.com/isService.json', {
			type : 'jsonp',
			method : 'get',
			onload : function(oRes) {
				result = oRes.json();
			}
		}).request({
			'productTypeCode' : 'COMIC',
			'originalProductId' : nbooksId
		});
		
		
		var oInterval = setInterval(function() {
			if (result) {
				if (result == 'true') {
					window.open('http://nstore.naver.com/comic/detail.nhn?originalProductId=' + nbooksId, '_blank');
				} else if (result == 'false') {
					alert('N스토어에 상품을 등록 중입니다. 잠시만 기다려주세요.');
				}
				clearInterval(oInterval);
				oInterval = null;
			}
		}, 100);
	}
	 
