	function applyFeed(){
		var params = {
		  popupType   : 'C',
		  callbackUrl  : '',
		  serviceId   : 'webtoon',
		  feedGroupId  : 'comic',
		  feedGroupName  : 'comic',
		  feedId    : ['551650'],
		  feedName   : ["오빠 왔다"],
		  feedUrl   : ["http://comic.naver.com/webtoon/list.nhn?titleId=551650"],
		  selected   : ['Y']
		};
		popupObject.popupFeedWindow(params,{charEnc:'UTF-8',feedPopupHost:'relation.commcast.naver.com'});
		
	}

