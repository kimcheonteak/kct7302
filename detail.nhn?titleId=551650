	 		$Element("other_content").className("other2");
		}
			 	
		if (false)
		{
			$Element("comic_before").css("visibility","hidden");
		}
		if (true)
		{
			$Element("comic_after").css("visibility","hidden");
		}

		new NavigationBar(navi_init,"/navigationToJson.nhn?ln=WEBTOON&titleId=551650&weekday=");
		
	   	new ExtraRemover();
	   	new KeyboardMover();
		// 공통댓글 페이지 로딩
		$('nCommentIframe').src = naver.comic.sNCommentUrl;
		
		
	    
	    naver.comic.htAdpostOption = {
	    	adpostUrl  : "http://cmapi.comic.naver.com/api/adpostRelay.nhn",
	    	adpostChannel : "nwebtoon_28d58a4758ca412d916b696dbd24f577",
	    	adpostCount : 12,
	    	adpostKeyword : "",
	    	nBuffer : 1000
	    };
	    new Adpost(jindo.$("adpostArea"), 
			   		   jindo.$Template("adpostTemplate"), 
		   		   	naver.comic.htAdpostOption.adpostUrl, 
		   		   	naver.comic.htAdpostOption.adpostChannel, 
		   		   naver.comic.htAdpostOption.adpostCount, 
		   		   naver.comic.htAdpostOption.adpostKeyword, {
        				nBuffer : 1000
        });	     
        		
        // 이미지 실제 위치
        webtoonContentActualTopMargin = $Element($Element('comic_view_area').query('img')).offset().top ;

        // PPL
        if ('true' == 'true') {
        	new NDPPPL($('ppl_section'), $('ppl_link_tag'), '1126T', '551650', 'false', 'DEFAULT');
        }
        
		//리모콘.
		var elComicRemocon = $("comicRemocon");
		if ( readCookie("visibleRemocon") != null &&  !readCookie("visibleRemocon") == "false" ){
			$("comicRemocon").style.display = 'block';	
		}
        var remoconOffset = $Element("comicRemocon").offset();
        var initLeft = remoconOffset.left;
        var initTop = remoconOffset.top;
        
        if ( initLeft == 0 && initTop == 0 && !(elComicRemocon.offsetLeft == 0 && elComicRemocon.offsetTop == 0)){
        	initLeft = elComicRemocon.offsetLeft;
        	initTop = elComicRemocon.offsetTop;
        }
	  	
	  	var height = 186;
	  	
	  	var oClientSize = $Document(document).clientSize();
	  	var nDifference = oClientSize.height - (height + initTop);
	  	if ( nDifference < 0  ){
	  		nDifference = Math.abs(nDifference);	
	  		initTop =  initTop - nDifference - 3;
	  	}
	  	
	  	// 초기값 체크

		eraseCookie("top");
		eraseCookie("left");
        var remocon ;
        if(isIE6HighOrOtherBrowser()){
	       	remocon = new RemoconComic("comicRemocon",height);
        }else{
        	remocon = new LowIE6RemoconComic("comicRemocon",height); 
        }
        
        var drag = new Drag("comicRemocon",remocon,initLeft,initTop);
        $Element("comicRemocon").css("top", initTop + "px");
	}
	
	function goUp(){
	    var max = $Element('comic_view_area').offset().top;
	    window.scrollTo(0,0);
	}

	function goDown(){
    	var max =  $Element('comic_view_area').first().height();
	    window.scrollTo(0,max-($Element("comicRemocon").height()));
    }

    function goPainterListByPainterId(painterId) {
        location.href = "/paintersTitle.nhn?painterId=" + encodeURIComponent(painterId);
    }

    function doComicReport(){
        try {
			var userId = "";
	        var painterId = "모나 / 모나";

	        if (!isLogin) {
                throw "NL"; // 로그인이 필요.
            } 
	
			if(userId == painterId) {
				throw "EP"; // 본인글은 신고할 수 없음.
			}

	    	var form = document.reportForm;
			var request_report = open("about:blank", "request_report", "top=10,left=10,toolbar=no,location=0,directories=0,status=yes,menubar=0,scrollbars=no,resizable=0,width=606,height=491");
			form.target = "request_report";
			form.submit();
        } catch (e) {
            if(e == "NL") {
            	if( confirm('로그인 해야 이용할 수 있습니다. 로그인 하시겠습니까?') ) {
                    location.href = "https://nid.naver.com/nidlogin.login?mode=form&url="+encodeURIComponent(location.href);
                }
            } else if(e == "EP") {
            	alert("본인의 게시물을 신고할 수 없습니다.");
            }
        } finally {
            return false;
        }
    }
    
