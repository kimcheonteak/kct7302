	function copyThisURL ()	{
		if ( window.clipboardData) {
 			$Ajax("http://comic.naver.com/webtoon/copyUrl.nhn").request({"titleId":"551650","no":"208"});
 			window.clipboardData.setData('Text','http://comic.naver.com/webtoon/detail.nhn?titleId=551650&no=208' );		 
 			return true;
		} else {
			alert('복사 기능을 지원하지 않는 브라우저입니다.');
			return false;
		}
	}	    	
