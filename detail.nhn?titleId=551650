<script type="text/javascript">
var agent = navigator.userAgent.toLowerCase();

function loadEvent(){
	try{
		getGNB();
	}catch(ex){
		
	}
	new naver.comic.RealtimeRankLayer();
	if ( $("uploadLinkId") ){
		$Fn(naver.comic.oEventHandler.goUpload).attach($("uploadLinkId"),"click");
	}
	naver.comic.oArtistAction = new naver.comic.Artist();
	artistAction = naver.comic.oArtistAction;
	naver.comic.oSearchBar = new naver.comic.SearchBarOptionActHandler();

	
	naver.comic.oRollingTextBanner = new naver.comic.TextBanner();
	naver.comic.oRollingTextBanner.start();
	$Fn(function(oEvent ) {
		naver.comic.oRollingTextBanner.stop();
		oEvent.stop();
	}).attach($$.getSingle(".rolling"),"mouseover");
	$Fn(function(oEvent ) {
		if( !naver.comic.oRollingTextBanner.getRegister()  ){
			naver.comic.oRollingTextBanner.start();
		}
		oEvent.stop();
	}).attach($$.getSingle(".rolling"),"mouseout");		
	
	
	// GNB 이벤트 재정의.
	$Fn(function(oEvent){
		var aWtChild = $Element("gnb" ).child(function (v){return true;});
		var wtSrc = $Element(oEvent.element);
		var bContain = false;
		for (var i = 0 ; i < aWtChild.length ;i++){
			if ( aWtChild[i].$value() == wtSrc.$value()){
				bContain = true;
				break;				
			}
		}
		if ( !bContain && gnbAllLayerClose ){
			gnbAllLayerClose();		
			
		}	

	}).attach(document,"click");
}			
$Fn(loadEvent).attach(window,'load');
</script>

    

