<script type="text/template" id="adpostTemplate">
<div class="type{=length}">
	{for num:adpost in adpostList}
	{if num == 0}
		{set nclick = 'alist'}
	{/if}
	{if num == 1}
		{set nclick = 'blist'}
	{/if}
	{if num == 2}
		{set nclick = 'clist'}
	{/if}
	{if num == 3}
		{set nclick = 'dlist'}
	{/if}
