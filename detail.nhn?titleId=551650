<script type="text/javascript">
function toogleAlert() {
	var alertToolTip = $("alertToolTip");
	if(alertToolTip.style.display == "none") {
		alertToolTip.style.display = "block";
	} else {
		alertToolTip.style.display = "none";
	}
}

